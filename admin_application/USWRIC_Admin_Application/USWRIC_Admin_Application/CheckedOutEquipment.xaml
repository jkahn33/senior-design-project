<Page x:Class="USWRIC_Admin_Application.CheckedOutEquipmentPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:USWRIC_Admin_Application"
      mc:Ignorable="d" 
      Height="Auto" Width="Auto"
      Title="CheckedOutEquipment">
    <Viewbox>
        <Grid Height="450" Width="800">
            <Label Content="Current Checked Out Equipment" HorizontalContentAlignment="Center" HorizontalAlignment="Left" Margin="214,50,0,0" VerticalAlignment="Top" Height="35" Width="372" FontFamily="Arial Black" FontSize="20"/>
            <DataGrid Name="CheckedOutEquipmentGrid" ItemsSource="{Binding}" AutoGenerateColumns="True" IsReadOnly="True" ColumnWidth="*" CanUserAddRows="False" Margin="0,85,0,34.6"/>
            <Button Name="CheckedOutBack" Content="Back" HorizontalAlignment="Left" Margin="362,420,0,0" VerticalAlignment="Top" Width="75" Click="CheckedOutBack_Click"/>
        </Grid>
    </Viewbox>
</Page>
